  - name: DPI_REQ_ERR_SKIP_COMP
    title: DPI Instruction Queue Skip Completion Register
    address: 0x1DF0000000838
    bus: NCB
    fields:
      - name: --
        bits: 63..24
        access: RAZ
        reset: --
        typical: --
