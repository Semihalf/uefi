      - name: LMAC_ID
        bits: 13..12
        access: RO/H
        reset: 0x0
        typical: --
        description: Logged LMAC_ID associated with NXC exceptions

      - name: CHANNEL
        bits: 11..0
        access: RO/H
        reset: 0x0
        typical: --
        description: Logged channel for NXC exceptions


  - name: BGX(0..5)_CMR_BAD
    address: 0x11800E0001020 + a*0x1000000
    bus: RSL
    fields:
      - name: --
        bits: 63..1
        access: ---
        reset: --
        typical: --
        description: Reserved.

      - name: RXB_NXL
        bits: 0
