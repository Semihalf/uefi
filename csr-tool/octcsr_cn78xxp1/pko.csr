  - name: PKO_L2_SQ(0..511)_SHAPE
    title: PSE Level 2 Shaping Control
    address: 0x1540000080010 + a*0x200
    bus: NCB
    fields:
      - name: --
        bits: 63..25
        access: ---
        reset: --
        typical: --
