        reset: 0x0
        typical: --
        description: Debug select for LUE's mux 3.

      - name: --
        bits: 55
        access: RAZ
        reset: --
        typical: --
        description: Reserved.

      - name: SEL2
        bits: 54..48
        access: R/W
        reset: 0x0
        typical: --
        description: Debug select for LUE's mux 2.

