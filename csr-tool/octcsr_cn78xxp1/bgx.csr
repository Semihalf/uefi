      - name: PTIME
        bits: 15..0
        access: R/W
        reset: 0x0
        typical: --
        description: Back off the TX bus for (PTIME * 512) bit-times.


  - name: BGX(0..5)_GMP_GMI_TX(0..3)_PAUSE_TOGO
    title: BGX GMI TX Time-to-Backpressure Registers
    address: 0x11800E0038258 + a*0x1000000 + b*0x100000
    bus: RSL
    attributes:
      subblock: "GMP"
