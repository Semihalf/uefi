  - name: PKI_STYLE(0..63)_TAG_MASK
    title: PKI Per-Style Tag Generation Mask Registers
    address: 0x1180044021000 | a<<3
    bus: RSL
    internal: |
      INTERNAL: This register is in BE.
    attributes:
      arch_max: "256"
    fields:
      - name: --
