	DDR-CONFIG-SPD-ADDR.DIMM0.LMC3.N0 = "0x1056";
	DDR-CONFIG-SPD-ADDR.DIMM1.LMC2.N0 = "0x1055";
	DDR-CONFIG-SPD-ADDR.DIMM0.LMC2.N0 = "0x1054";
	DDR-CONFIG-SPD-ADDR.DIMM1.LMC1.N0 = "0x1053";
	DDR-CONFIG-SPD-ADDR.DIMM0.LMC1.N0 = "0x1052";
	DDR-CONFIG-SPD-ADDR.DIMM1.LMC0.N0 = "0x1051";
	DDR-CONFIG-SPD-ADDR.DIMM0.LMC0.N0 = "0x1050";

	/* DIMM SPD data to be used if memory doesn't support the standard
