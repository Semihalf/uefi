  - name: BGX(0..5)_GMP_GMI_TX(0..3)_PAUSE_PKT_TIME
    title: BGX GMI TX PAUSE Packet PAUSE-Time Registers
    address: 0x11800E0038238 + a*0x1000000 + b*0x100000
    bus: RSL
    attributes:
      exempt_name_length: "True"
      subblock: "GMP"
