      - name: --
        bits: 63..16
        access: RAZ
        reset: --
        typical: --
        description: Reserved.

      - name: PTIME
        bits: 15..0
        access: RO/H
        reset: --
        typical: --
        description: Amount of time remaining to backpressure, from the standard 802.3 PAUSE timer.


  - name: BGX(0..5)_GMP_GMI_TX(0..3)_PAUSE_ZERO
    title: BGX GMI TX PAUSE-Zero-Enable Registers
    address: 0x11800E0038260 + a*0x1000000 + b*0x100000
    bus: RSL
    attributes:
      subblock: "GMP"
