        reset: --
        typical: --
        description: Reserved.

      - name: VMA_FINE_CFG_SEL
        bits: 15
        access: R/W/H
        reset: --
        typical: --
        description: |
          Recommended settings:
          1 = Enabled. Fine step adaptation selected (10.3125 Gbaud rate).
          0 = Disabled. Coarse step adaptation selected (rates lower than 10.3125 Gbaud).

      - name: VMA_MM
        bits: 14
