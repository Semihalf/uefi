      - name: --
        bits: 23..17
        access: RAZ
        reset: --
        typical: --
        description: Reserved.

      - name: LOP_TXB_ECC_DIS
        bits: 16
        access: R/W
        reset: 0
        typical: 0
        description: Disable ECC for LOP TXBUFF data memory.

      - name: --
