	DDR-CONFIG-DQX-CTL.RANKS1.DIMMS2.LMC1.N0 = "0x4";
	DDR-CONFIG-DQX-CTL.RANKS4.DIMMS1.LMC1.N0 = "0x4";
	DDR-CONFIG-DQX-CTL.RANKS2.DIMMS1.LMC1.N0 = "0x4";
	DDR-CONFIG-DQX-CTL.RANKS1.DIMMS1.LMC1.N0 = "0x4";
	DDR-CONFIG-DQX-CTL.RANKS2.DIMMS2.LMC0.N0 = "0x4";
	DDR-CONFIG-DQX-CTL.RANKS1.DIMMS2.LMC0.N0 = "0x4";
