        bits: 63..15
        access: ---
        reset: --
        typical: --
        description: Reserved.

      - name: S_SPARE
        bits: 14..13
        access: R/W/H
        reset: 0x0
        typical: 0x0
        description: |
          bit 14: enable rx1_div33 clock in the SerDes
          bit 13: enable rx0_div33 clock in the SerDes

      - name: LDLL_ISEL
        bits: 12..11
        access: R/W/H
        reset: 0x0
