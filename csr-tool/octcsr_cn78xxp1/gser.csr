          R_8G_REFCLK100:           0x23
          R_125G_REFCLK15625_KX:    0x1E
          R_3125G_REFCLK15625_XAUI: 0x1E
          R_103125G_REFCLK15625_KR: 0xF
          R_125G_REFCLK15625_SGMII: 0x1E
          R_5G_REFCLK15625_QSGMII:  0x1E
          R_625G_REFCLK15625_RXAUI: 0x14
          R_25G_REFCLK125:          0x14
          R_5G_REFCLK125:           0x14
          R_8G_REFCLK125:           0x23


