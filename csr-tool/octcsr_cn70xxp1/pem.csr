      - name: --
        bits: 40..0
        access: ---
        reset: --
        typical: --
        description: Reserved.


  - name: PEM(0..1)_CPL_LUT_VALID
    title: PEM Cmpletion Lookup Table Valid
    address: 0x11800C0000098 + a*0x1000000
    bus: RSL
    description: Bit set for outstanding tag read.
    fields:
      - name: --
        bits: 63..32
        access: ---
        reset: --
        typical: --
        description: Reserved.

      - name: TAG
        bits: 31..0
        access: RO/H
        reset: 0x0
        typical: 0x0
        description: |
          Bit vector set cooresponds to an outstanding tag
          expecting a completion.


