  - name: SLI_PKT_IN_DONE(0..63)_CNTS
    address: 0x11F0000012000 + a*0x10
    bus: PEXP_NCB
    description: |
      "SLI_PKT_IN_DONE[0..63]_CNTS = SLI Instruction Done ring# Counts
      Counters for instructions completed on Input rings."
    fields:
      - name: --
        bits: 63..32
