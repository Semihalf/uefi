	DDR-CONFIG-MODE1-RTT-WR.RANKS2.DIMMS1.RANK0.LMC3.N0 = "0x2";
	DDR-CONFIG-MODE1-RTT-WR.RANKS2.DIMMS2.RANK3.LMC2.N0 = "0x2";
	DDR-CONFIG-MODE1-RTT-WR.RANKS2.DIMMS2.RANK2.LMC2.N0 = "0x2";
