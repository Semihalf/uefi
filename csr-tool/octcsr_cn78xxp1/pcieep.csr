        access: RO/H
        reset: 0
        typical: 0
        description: Received queue not empty. Indicates there is data in one or more of the receive buffers.

      - name: TRBNE
        bits: 1
        access: RO/H
        reset: 0
        typical: 0
        description: Transmit retry buffer not empty. Indicates that there is data in the transmit retry buffer.

      - name: RTLPFCCNR
